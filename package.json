{
  "name": "baseerr",
  "version": "2.0.1",
  "description": "A solid BaseError class that supports custom properties and wrapping errors",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs"
    }
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run build-ts && npm run _build_esm_rename && npm run _build_cjs_rename",
    "build-ts": "tsc -p tsconfig.json && tsc -p tsconfig-cjs.json",
    "_build_esm_rename": "mv dist/esm/index.js dist/esm/index.mjs && mv dist/esm/index.js.map dist/esm/index.mjs.map",
    "_build_cjs_rename": "mv dist/cjs/index.js dist/cjs/index.cjs && mv dist/cjs/index.js.map dist/cjs/index.cjs.map",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjmehta/baseerr.git"
  },
  "keywords": [
    "base error",
    "custom error",
    "base error class",
    "custom error class",
    "base",
    "custom",
    "error",
    "class",
    "wrap",
    "data"
  ],
  "author": "Tejesh Mehta",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tjmehta/baseerr/issues"
  },
  "homepage": "https://github.com/tjmehta/baseerr#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-typescript": "^7.10.1",
    "@types/jest": "^25.2.3",
    "babel-core": "^6.26.3",
    "babel-jest": "^26.0.1",
    "escape-string-regexp": "^4.0.0",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "fast-safe-stringify": "^2.0.7"
  }
}
